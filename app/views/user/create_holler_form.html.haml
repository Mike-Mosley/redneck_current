#registration
  = render :partial => 'tabs'
  #registration_table{:style => "max-width:700px"}
    #settings
      = form_for(@holler, :url => {:action => 'save_holler'}) do |f|
        = f.error_messages
        %table#table_settings
          %tbody
            %tr
              %th Page Title:
              %td= f.text_field :title
            %tr
              %th Page Category
              %td= f.text_field :category_name
            %tr
              %th Post Setting:
              %td= f.select :post_settings, Holler::POSTSETTINGs.collect
        = f.submit 'Save Settings'
